{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Template for frontend applications based on ReactJS",
  "main": "index.js",
  "scripts": {
    "lint:css": "`npm bin`/stylelint \"src/**/*.css\" --formatter verbose",
    "lint:js": "`npm bin`/eslint src --ext .ts --ext .tsx --quiet",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "NODE_ENV=development webpack serve --config webpack.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SMhero/react-template.git"
  },
